<table border = 2>
<thead>
<tr>
<th>ID</th>
<th>Title</th>
<th>Description</th>
<th>Created At</th>
<th>Updated At </th>
<th colspan = 3>Actions </th>
<tr>
</thead>
<tbody>
<% @art.each do |article| %>
    <tr>
    <th><%= article.id %> </th>
    <th><%= article.title %> </th>
    <th><%= article.description %> </th>
    <th><%= article.created_at %> </th>
    <th><%= article.updated_at %> </th>
    <th> Placeholder </th>
    </tr>
    <% end %>
</tbody>
</table>